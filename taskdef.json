{
    "family": "ecs-nginx-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::975050220365:role/ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "containerDefinitions": [
        {
            "name": "nginx-container",
            "image": "975050220365.dkr.ecr.ap-south-1.amazonaws.com/sekhar/ecscicd:t3",
            "memory": 512,
            "cpu": 256,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}